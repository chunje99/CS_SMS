        <TextBlock HorizontalAlignment="Left" Margin="600,100,0,0" Text="Disconected" TextWrapping="Wrap" VerticalAlignment="Top" Name="Print0_Status"/>
        <Button Content="연결 테스트" Margin="300,200,0,0" VerticalAlignment="Top" Width="130" Click="Connect_Click" Tag="0"/>
        <Button Content="테스트 인쇄" Margin="450,200,0,0" VerticalAlignment="Top" Width="130" Click="Sample_Print_Click" Tag="0"/>
        <Button Content="슈트QR 인쇄" Margin="600,200,0,0" VerticalAlignment="Top" Width="130" Click="Chute_Print_Click" Tag="0"/>
